pipeline {
  agent any
  stages {
    stage('Dev Build') {
      steps {
        echo 'Git pull code from repo'
      }
    }

    stage('Smoke Test') {
      steps {
        echo 'Print smoke test'
      }
    }

    stage('Cerify') {
      steps {
        echo 'Certify the build!!'
      }
    }

  }
}